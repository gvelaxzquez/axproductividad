
<div class="modal" id="ModalListaActividades" tabindex="-1" role="dialog" aria-labelledby="largeModalHead" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" ><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="tDetalleTV">Detalle valor ganado</h4>
            </div>
            <div class="modal-body">

                <div class="row">


                    @*<div class="col-md-2">

            </div>*@
                    <div class="col-md-6">
                        <div class="tile tile-default " id="" title="Cantidad de tareas" style="cursor:pointer;">
                            <div class="title tile-default " id="LblTotalDTDTV"></div>
                            <p>Tareas</p>

                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="tile tile-default " id="tLblProductividadM" title="Valor ganado total" style="cursor:pointer;">
                            <div class="title tile-default " id="LblTotalDTDHV"></div>
                            <p>Horas</p>

                        </div>
                    </div>

                    @*<div class="col-md-2">

            </div>*@


                </div>

                <!--inicia-->
                <div class="row">
                    <div class="table-responsive">
                        <table class="table datatable text" id="TblLstActividades">
                            <thead>
                                <tr>
                                    <th width="15%">#</th>
                                    <th>Descripción</th>
                                    <th>Fecha fin</th>
                                    <th>Valor ganado</th>
                                    <th>Estatus</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>

                    </div>
                </div>
                <!--termina-->
            </div>
        </div>
    </div>
</div>


<input type="hidden" id="urlConsultaActividadesTerminadasDia" value="@Url.Action("ConsultaActividadesTerminadasDia","Actividades")" />