<body>
    <input id="txtDeGera" type=password style='width:0px;position:absolute;right:-100px'>
    @{
        if (environment == "Development")
        {
            @Html.Raw(@"
<script type=""module"">
import RefreshRuntime from 'http://localhost:5173/@react-refresh';
RefreshRuntime.injectIntoGlobalHook(window);
window.$RefreshReg$ = () => { };
window.$RefreshSig$ = () => (type) => type;
window.__vite_plugin_react_preamble_installed__ = true;
</script>
<script type=""module"" src=""http://localhost:5173/@vite/client""></script>
")
        }
    }

    @{
        var asistencia = ((bool)Session["Asistencia" + Session.SessionID]);

        if (asistencia)
        {

            <nav class="social">
                <ul>
                    <li onclick="CapturaAsistencia();"><a href="#"><i class="fa fa-clock-o"></i>  Asistencia</a></li>
                </ul>
            </nav>
        }

    }




    <div class="page-container">

        @*<div class="page-container page-navigation-toggled page-container-wide">*@
        <div class="page-sidebar" style="background-color:#F9FBFD">
            <ul class="x-navigation">
                @*<ul class="x-navigation x-navigation-minimized">*@
                <li class="xn-logo xn-principal ">

                    @{

                        var home = ((string)Session["Home" + Session.SessionID]);


                        <a href=@home></a>
                    }
                    <a href="#" class="x-navigation-control xn-principal"></a>
                </li>
                <li class="xn-title">

                    @{
                        var Userd = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;

                        if (Userd.CantOrg > 0)
                        {
                            <button class="btn btn-default btn-grid" id="BtnOrganizacion"> <span class="fa fa-refresh"></span> </button>
                        }

                        @Userd.DescripcionDepartamento


                    }

                </li>

                @*Proyectos*@

                @*{

                        var U = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;
                        if (U.IdTipoUsuario == 15 || U.IdTipoUsuario == 17)
                        {



                            <li class="xn-openable">

                                <a style="cursor:pointer" href="#" title="Proyectos recientes"><span class="fa fa-folder-open"></span> <span class="xn-text">Mis Proyectos</span></a>
                                <ul>

                                    @{ foreach (var i in LstProyectos)
                                        {

                                            <li><a style="cursor:pointer" href="@Url.Action("Proyecto", "Proyectos", new { Id = i.Clave })" title="@i.Nombre"><span class="fa fa-share"></span> <span>@i.Nombre</span></a></li>


                                        }
                                    }
                                </ul>
                            </li>



                        }
                    }


                    @*Menú*@

                @{
                    var lstMenu = ((Sesion)Session["usuario" + Session.SessionID]).Menu;
                    var lstMenuPadres = lstMenu.Where(menu => menu.IdMenuPadre == null).OrderBy(id => id.Orden).ToList();

                    foreach (var item in lstMenuPadres)
                    {
                        var hijos = lstMenu.Where(subMenu => subMenu.IdMenuPadre == item.IdMenu).OrderBy(id => id.Orden).ToList();

                        if (hijos.Count > 0)
                        {
                            <li class="xn-openable">

                                <a href="#"><span class="@item.Icono"></span> <span class="xn-text">@item.Descripcion</span></a>
                                <ul>

                                    @{ foreach (var hijo in hijos)
                                        {
                                            <li><a href="@Url.Action(@hijo.Accion, @hijo.Controlador)"><span class="@hijo.Icono"></span>@hijo.Descripcion</a></li>
                                        }

                                        if (item.Descripcion == "Querys")
                                        {
                                            var Querys = ((Sesion)Session["usuario" + Session.SessionID]).Querys.ToList();
                                            foreach (var y in Querys)
                                            {

                                                <li><a style="cursor:pointer" href="@Url.Action("q", "Querys", new { Id = y.IdUnique })" title="@y.Nombre" target="_blank"><span class="fa fa-flash"></span> <span>@y.Nombre</span></a></li>


                                            }
                                        }


                                        if (item.Descripcion == "Workspace")
                                        {
                                            var WS = ((Sesion)Session["usuario" + Session.SessionID]).Workspaces.ToList();
                                            foreach (var w in WS)
                                            {

                                                <li><a style="cursor:pointer" href="@Url.Action("w", "Workspace", new { Id = w.IdUnique })" title="@w.Nombre"> <span>@w.Nombre</span></a></li>


                                            }
                                        }
                                    }



                                </ul>
                            </li>
                        }
                        else
                        {

                            <li><a href="@Url.Action(@item.Accion, @item.Controlador)" title="@item.Descripcion"><span class="@item.Icono"></span> <span class="xn-text">@item.Descripcion</span></a></li>

                        }
                    }

                }






            </ul>

        </div>

        <div class="page-content" style="height: auto !important">

            <ul class="x-navigation x-navigation-horizontal x-navigation-panel">
                <li class="xn-icon-button xn-principal">
                    <a href="#" class="x-navigation-minimize xn-principal" id="BMinimizar"><span class="fa fa-dedent"></span></a>
                </li>
                <li class="xn-icon-button pull-right xn-principal">
                    <a href="#" class="mb-control xn-principal" data-box="#mb-signout"><span class="fa fa-sign-out"></span></a>
                </li>
                <li class="xn-icon-user pull-right xn-principal hidden-xs" style="padding:0px;">


                    @{
                        Random rndl = new Random();


                        var User = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;
                        var imagen = User.NumEmpleado + ".jpg?" + rndl.Next();


                        <a class="mb-control xn-principal" style="padding:0px;cursor:pointer;" onclick="ConsultaPerfil('@User.NumEmpleado')">
                            <img class="img-circle" style="width:50px; height:50px;" src="~/Archivos/Fotos/@imagen" /> @User.NombreCompleto

                        </a>


                    }


                </li>


                <li class="xn-icon-button pull-right xn-principal" title="Cambiar contraseña">
                    <a href="#" class="mb-control btn-CambiarPass xn-principal"><span class="fa fa-key"></span></a>
                </li>





                @{

                    var User2 = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;
                    if (User2.Captura)
                    {
                        <li class="xn-icon-button pull-right hidden-xs">
                            <a href="#" class="xn-principal"><span class="fa fa-plus-circle"></span></a>
                            <div class="panel panel-primary animated zoomIn xn-drop-left xn-panel-dragging">
                                <div class="panel-body list-group list-group-contacts">
                                    @{
                                        var Workitems = ((Sesion)Session["usuario" + Session.SessionID]).WorkItems;
                                        foreach (var item in Workitems)
                                        {
                                            <a href="#" class="list-group-item" onclick="newItem(@item.ActividadTipoId);">
                                                <img src="~/Content/Project/Imagenes/@item.Url" style="height:24px; width:24px;" class="pull-left" alt="@item.Nombre">
                                                <span class="contacts-title">@item.Nombre</span>
                                            </a>
                                        }
                                    }
                                </div>
                            </div>
                        </li>
                    }

                }


                <li class="xn-icon-button pull-right xn-principal" title="Notificaciones">
                    <a href="#" class="mb-control  xn-principal ts-button"><span class="fa fa-bell-o"></span></a>
                    <div class="informer informer-danger" id="NoNotif"></div>
                </li>


                <li class="xn-icon-button pull-right hidden" id="list-tracking-clock" style="height: 50px; width:50px">


                    <a href="#" class="xn-principal dropdown-toggle" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><span class="fa fa-clock-o"></span></a>
                    <div class="informer informer-danger" style="right: -5px" id="count-tracking-clock"></div>

                    <div id="clock-tracking" class="dropdown-menu" aria-labelledby="dropdownMenu2"
                         style="left: -95px; padding: 15px; width: 250px; max-height: 350px; overflow-x: auto;">
                    </div>

                </li>


                <li class="hidden-xs">

                    <h3 style="margin-top:20px;" class="" id="hTitle"> @ViewBag.Title </h3>



                </li>






                @{

                    var UserS = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;
                    if (UserS.IdTipoUsuario != 19)
                    {

                        <li class="xn-icon-button pull-right hidden-xs" title="Capturar actividad">
                            <a href="#" class="xn-principal search-icon" aria-expanded="false"><span class="fa fa-search "></span></a>

                        </li>


                        <li class="hidden-xs pull-right xn-icon-button">
                            <div id="divsel" style="padding: 10px 0px;">
                                <select style="width:500px;" multiple="multiple" class="hidden" id="SelActividadSearch"></select>
                            </div>
                        </li>
                    }

                }





            </ul>


            <div class="mensajes" style="z-index: 9999; position: absolute"></div>
            <div class="mensajesConfirmacion" style="z-index: 9999; position: absolute"></div>
            <div class="page-content-wrap">

                @RenderBody()


            </div>
        </div>
    </div>
    <div class="message-box animated fadeIn" data-sound="alert" id="mb-signout">
        <div class="mb-container">
            <div class="mb-middle">
                <div class="mb-title"><span class="fa fa-sign-out"></span>¿Deseas <strong>Salir</strong> ?</div>

                <div class="mb-footer">
                    <div class="pull-right">
                        <a href=@Url.Action("Index", "Login") class="btn btn-success btn-lg">Si</a>
                        <button class="btn btn-default btn-lg mb-control-close">No</button>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="theme-settings ">
        @*<div class="ts-button"><span class="fa fa-cog"></span></div>*@


        <div class="ts-body">
            <div class="ts-title">

                <button type="button" id="BtnCloseNotif" class="close pull-left" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>

                <div class="btn-group pull-right">
                    <a class="btn btn-info" id="BtnVerInternas">Internas</a>
                    <a class="btn btn-default" id="BtnVerExternas">Clientes</a>
                </div>

            </div>

            <div class="ts-row" style="padding-top:10px" id="">
                <div class="messages messages-img" id="NotInternas" style="height:950px; overflow:scroll;background-color:#FFF;padding-top:10px;">
                </div>
                <div class="messages messages-img" id="NotExternas" style="height: 950px; overflow: scroll; background-color: #FFF;padding-top:10px;">
                </div>
            </div>


        </div>
    </div>

    <!-- Modal cambio contraseña-->
    <div class="modal" id="ModalContrasenia" tabindex="-1" role="dialog" aria-labelledby="defModalHead" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" id="BtnCerrarPass" class="close btn-PassClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                    <h4 class="modal-title" id="defModalHead">Cambio de contraseña</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="form-group">

                            <div class="col-md-3 col-xs-14 text-left"><label class="control-label">Contraseña anterior:</label></div>
                            <div class="col-md-9 col-xs-14"><input type="password" class="form-control ReqPass" id="TxtContrasenaAnterior" item="Contraseña anterior" /> </div><br /><br />

                        </div>
                        <div class="form-group">

                            <div class="col-md-3 col-xs-14 text-left"><label class="control-label">Nueva contraseña:</label></div>
                            <div class="col-md-9 col-xs-14"><input type="password" class="form-control ReqPass" id="TxtContrasenaNueva" item="Nueva contraseña" /> </div><br /><br />

                        </div>
                        <div class="form-group">

                            <div class="col-md-3 col-xs-14 text-left"><label class="control-label">Confirmar contraseña:</label></div>
                            <div class="col-md-9 col-xs-14"><input type="password" class="form-control ReqPass" id="TxtConfirmarContrasena" item="Confirmación de contraseña" /> </div>

                        </div>

                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" id="BtnGuardarPass" class="btn btn-success btn-GuardarPass" data-dismiss="modal">Guardar<span class="fa fa-floppy-o fa-right"></span></button>
                </div>
            </div>
        </div>
    </div>



    <div class="modal" id="ModalFeels" tabindex="-1" role="dialog" aria-labelledby="defModalHead" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">

                @{
                    var User4 = ((Sesion)Session["usuario" + Session.SessionID]).Usuario;
                    <div class="modal-body">
                        <br />


                        <h4 class="control-label">Hola @User4.Nombre ¿Como te sientes hoy?</h4>
                        <div class="row">









                            <br />
                            <div class="col-md-12">
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFHappy" style="border-color:#FFF !important" onclick="SeleccionarUF(1, '#BtnUFHappy');">
                                        <img src="data:image/png;base64,">

                                    </button>
                                    <label class="text-center"> Feliz</label>
                                </div>
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFMotivated" style="border-color:#FFF !important" onclick="SeleccionarUF(2, '#BtnUFMotivated');">
                                        <img src="data:image/png;base64,">
                                    </button>
                                    <label class="text-center"> Motivado</label>
                                </div>
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFStress" style="border-color:#FFF !important" onclick="SeleccionarUF(7, '#BtnUFStress');">
                                        <img src="data:image/png;base64,">
                                    </button>
                                    <label class="text-center"> Estresado</label>
                                </div>
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFSad" style="border-color:#FFF !important" onclick="SeleccionarUF(4, '#BtnUFSad');">
                                        <img src="data:image/png;base64,">
                                    </button>
                                    <label class="text-center"> Triste</label>
                                </div>
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFTired" style="border-color:#FFF !important" onclick="SeleccionarUF(5, '#BtnUFTired');">

                                        <img src="data:image/png;base64,">
                                    </button>
                                    <label class="text-center"> Cansado</label>
                                </div>
                                <div class="col-md-2 text-center">
                                    <button class="btn btn-default btnUF" id="BtnUFAngry" style="border-color:#FFF !important" onclick="SeleccionarUF(6, '#BtnUFAngry');">
                                        <img src="data:image/png;base64,">
                                    </button>
                                    <label class="text-center"> Enojado</label>
                                </div>
                            </div>
                        </div>
                        <br />

                        @if (User4.Contesta == 1)
                        {
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="control-label"> <b>@User4.Pregunta</b> </h4>
                                    <input type="hidden" id="IdPregunta" value="@User4.IdPregunta" />

                                    <textarea class="form-control ReqF" style="min-width:100%" rows="5" id="TxtFComentariosUF" item="Respuesta" maxlength="3000"></textarea>
                                </div>
                            </div>

                        }
                        else
                        {
                            <input type="hidden" id="IdPregunta" value="0" />
                            <textarea class="hide" type="hidden" id="TxtFComentariosUF"></textarea>

                        }

                    </div>
                    <div class="modal-footer">
                        <button type="button" id="BtnGuardarFeels" class="btn btn-info btn-GuardaFeels btn-lg" data-dismiss="modal">ENVIAR </button>
                    </div>
                }
            </div>
        </div>
    </div>


