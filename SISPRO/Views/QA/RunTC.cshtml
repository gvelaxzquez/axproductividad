<div class="modal" id="MdlEjecutarCasoPrueba" tabindex="-1" role="dialog" aria-labelledby="largeModalHead" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog modal-lg" style="width:80%; height:80%">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" id="BtnCerrarBuscarCP"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="titleECP">Ejecutar caso de prueba</h4>
            </div>
            <div class="modal-body">
                <!--inicia-->

                <div class="row">

                    <div class="col-md-8 col-sm-12 col-xs-12">
                        @*<div style="padding-bottom: 10px;">


                            <h7 class="control-label separator">Título</h7>
                            <input type="text" style="max-width:100%" class="form-control " id="TxtTituloECP" item="Título" maxlength="500" disabled />

                        </div>*@

                        <div class="" style="border-bottom: none !important;">
                            <div class="panel-group accordion accordion-dc">
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h5 class="panel-title ">
                                            <a href="#accordion-DescripcionECP">
                                                Descripción
                                            </a>
                                        </h5>
                                    </div>
                                    <div class="panel-body panel-body-open" style="padding: 0px;" id="accordion-DescripcionECP">
                                        <div class="col-md-12" style="margin: 10px 0px" id="">

                                            <p class="form-control" style="min-width:100%; height:auto;" id="LblDescripcionECP"></p>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="" style="border-bottom: none !important;" id="">
                            <div class="panel-group accordion">
                                <div class="panel panel-default ">
                                    <div class="panel-heading">
                                        <h5 class="panel-title ">
                                            <a href="#accordion-EvidenciasECP">
                                                Evidencias
                                            </a>
                                        </h5>
                                    </div>
                                    <div class="panel-body " style="padding: 0px;" id="accordion-EvidenciasECP">
                                        <div class="col-md-12" style="margin: 10px 0px">

                                            @*<textarea class=" summernote form-control " style="min-width:100%" rows="25" id="TxtEvidenciasECP" item="Evidencia"></textarea>*@

                                            <textarea id="TxtEvidenciasECP" class="">

                                            </textarea>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>









                    </div>
                    <div class="col-md-4 col-sm-12 col-xs-12">

                        <div class="panel panel-default  projectcard" style="min-height:150px;">
                            <div class="panel-body profile">
                                <div class="row headerproyecto">
                                    <div class="col-md-12">
                                        <h5>Planeación</h5>
                                    </div>
                                </div>
                            </div>
                            <form class="form-horizontal">
                                <div class="panel-body">
                                    <div class="form-group" id="">
                                        <div class="col-md-1">

                                        </div>
                                        <label class="col-md-4 col-xs-5 control-label separator">Asignado</label>

                                        <div class="col-md-7 col-xs-7"><select class="form-control  selectpicker reqECP" data-live-search="true" id="SelUsuarioAsignadoECP" item="Usuario asignado"></select></div>
                                        @*<label class="col-md- col-xs-5 control-label separator" id="LblAsignadoECP"> </label>*@

                                    </div>
                                    <div class="form-group" id="">
                                        <div class="col-md-1">

                                        </div>
                                        <label class="col-md-4 col-xs-5 control-label separator">Clasificación</label>
                                        <label class="col-md-7 col-xs-5 control-label separator" id="LblClasificacionECP"> </label>

                                    </div>

                                    <div class="form-group" id="">
                                        <div class="col-md-1">

                                        </div>
                                        <label class="col-md-4 col-xs-5 control-label separator">T. Ejecución</label>
                                        <label class="col-md-7 col-xs-5 control-label separator" id="LblTiempoEjecucionECP"> </label>

                                    </div>



                                </div>
                            </form>


                        </div>

                        <div class="panel panel-default projectcard" style="min-height:350px;">
                            <div class="panel-body profile">
                                <div class="row headerproyecto">
                                    <div class="col-md-6">
                                        <h5>Resultado</h5>

                                    </div>
                                    <div class="col-md-6">

                                        <button type="button" class="btn btn-default pull-right" id="BtnGuardarECP"><span class="fa fa-floppy-o"></span></button>
                                    </div>
                                </div>
                            </div>
                            <form class="form-horizontal">
                                <div class="panel-body">
                                    <div class="form-group" id="">
                                        @*<label class="col-md-3 col-xs-5 control-label separator">Resultado</label>*@
                                        <div class="col-md-1">

                                        </div>
                                        <div class="col-md-10 col-xs-12">
                                            <select class="form-control  selectpicker reqECP" data-live-search="true" id="SelResultadoECP" item="Resultado">
                                                <option value='-1'>--Seleccionar--</option>
                                                <option value='L' data-content="<span><span class='fa fa-fw  fa-square text-success'> </span>Aprobado</span>"></option>
                                                <option value='B' data-content="<span><span class='fa fa-fw fa-square text-danger'> </span>Bloqueante</span>"></option>
                                                <option value='F' data-content="<span><span class='fa fa-fw fa-square text-warning'> </span>Fallido</span>"></option>
                                                <option value='A' data-content="<span><span class='fa fa-fw fa-square text-muted'> </span>Sin ejecutar</span>"></option>
                                                <option value='O' data-content="<span><span class='fa fa-fw fa-square text-info'> </span>Omitido</span>"></option>

                                            </select>
                                        </div>
                                        <div class="col-md-1">

                                        </div>
                                    </div>

                                    <div class="form-group" id="">
                                        <div class="col-md-1">

                                        </div>

                                        <label class="col-md-3 col-xs-5 control-label separator">Tiempo</label>
                                        <div class="col-md-7 col-xs-7">

                                            <div class="input-group date">
                                                <input type="text" id="TxtHorasFinalesECP" class="form-control  reqECP" item="Fecha inicio" disabled style="color:#000">
                                                <span class="input-group-addon " onclick="CapturarTrabajoActQA()"><span class="glyphicon glyphicon-plus-sign"></span></span>
                                            </div>

                                        </div>
                                    </div>

                                    <div id="" class="form-group">

                                        <div class="col-md-1">

                                        </div>

                                        <div class="col-md-10">
                                            <div class="separator">Defectos </div>

                                            <div style="position:relative; text-align: right">
                                                <i id="" onclick="newBugECP(7);" class="fa fa-plus-circle" style="margin-right: -3px; font-size: 14px; color: #3fbae4; cursor: pointer;"></i>

                                            </div>

                                            <div id="" class="divDefectosRel" style="min-height:150px;">

                                            </div>
                                        </div>



                                    </div>



                                </div>
                            </form>

                        </div>

                    </div>


                </div>

                <!--termina-->
            </div>


        </div>


    </div>
</div>


<input type="hidden" id="IdCicloCasoECP" value="0" />
<input type="hidden" id="IdActividadCaso" value="0" />
<input type="hidden" id="IdActividadEjecucionECP" value="0" />

<input type="hidden" id="IdProyectoECP" value="0" />

<input type="hidden" id="urlConsultaECP" value="@Url.Action("ConsultaEjecucionCaso", "QA")" />
<input type="hidden" id="urlGuardarECP" value="@Url.Action("GuardarResultadoCasoCiclo", "QA")" />



@{
    Random rnd = new Random();
    <script src="~/Scripts/Project/CasoPruebaEjecutar.js?@rnd.Next()"></script>


}
