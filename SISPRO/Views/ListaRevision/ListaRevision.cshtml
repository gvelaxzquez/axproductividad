                <h4 class="modal-title" id="largeModalHead">Copiar Lista de Revisión</h4>
            </div>
            <div class="modal-body">
                <!--inicia-->
                <div class="row">
                    <div class="col-md-12">
                        <form class="form-horizontal">
                            <div style="position: absolute; right: 35px; z-index: 100;">
                                <button type="button" class="btn btn-success btnGuardar" id="">Guardar<span class="fa fa-floppy-o fa-right"></span></button>
                            </div>
                            <div class="form-group col-md-12">
                                <label for="selProceso" class="col-md-2 control-label">Proyecto</label>
