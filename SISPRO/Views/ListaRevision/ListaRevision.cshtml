                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </form>
                    </div>
                </div>
                <!--termina-->
            </div>
        </div>
    </div>
    @Html.Hidden("LeerComboProyecto", Url.Action("LeerComboProyecto", "Auditoria"))
    @Html.Hidden("CopiarListaRevisionDetalle", Url.Action("CopiarListaRevisionDetalle", "ListaRevision"))
</div>

<div class="modal" id="mdlImportarListaRevision" tabindex="-1" role="dialog" aria-labelledby="defModalHead" aria-hidden="true" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close btn-PassClose" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="defModalHead">Importar Listas de Revisión</h4>
            </div>
            <br />
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <label class="col-md-3 col-xs-12 control-label">Archivo (.XSLX)</label>
                    </div>
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <input accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" type="file" class="fileinput btn-primary fa fa-upload fileImportar" name="files[]" id="" title=" Cargar" />
                            <span class="help-block"></span>
                        </div>
                    </div>
                    <br />
                    <div class="col-md-12">
                        <div class="row">
                            <div>
                                <div class="col-md-2 col-xs-12">
                                    <label class="control-label" for="rdo1"><input type="radio" id="rdoCarga" name="TipoCarga" value="1" checked /> Carga</label>
                                </div>
                                <div class="col-md-3 col-xs-12">
                                    <label class="control-label" for="rdo2"><input type="radio" id="rdoActualizacion" name="TipoCarga" value="2" /> Actualización</label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="col-md-12"><hr /></div>
                    <div class="col-md-12">
                        <div class="col-md-12 col-xs-12 text-left"><label class="control-label">Descargar <a href="~/Archivos/Formatos/Layout_Checklist.xlsx"> ejemplo formato XSLX </a> para ver el formato requerido.</label></div>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" id="" class="btn btn-success btnImportar"><span class="fa fa-arrow-circle-up"></span> Importar</button>
            </div>
        </div>
    </div>ImportarListaRevision
    @Html.Hidden("ImportarListaRevision", Url.Action("ImportarListaRevision", "ListaRevision"))
</div>